    {
      "name": "server",
      "default": "127.0.0.1:5536",
      "optional": false,
      "control": "text",
      "type": "string",
      "hint": {
        "en_US": "The url of the RabbitMq server",
        "zh_CN": "RabbitMq 服务器的 URL"
      },
      "label": {
        "en_US": "Server address",
        "zh_CN": "服务器地址"
      }
    },
    {
      "name": "username",
      "default": "root",
      "optional": false,
      "control": "text",
      "type": "string",
      "hint": {
        "en_US": "The username of the RabbitMq server",
        "zh_CN": "RabbitMq 服务器的用户名"
      },
      "label": {
        "en_US": "username",
        "zh_CN": "用户名"
      }
    },
    {
      "name": "password",
      "default": "abc-123",
      "optional": false,
      "control": "text",
      "type": "string",
      "hint": {
        "en_US": "The password of the RabbitMq server",
        "zh_CN": "RabbitMq 服务器的密码"
      },
      "label": {
        "en_US": "password",
        "zh_CN": "密码"
      }
    },
    {
      "name": "vhost",
      "default": "/",
      "optional": false,
      "control": "text",
      "type": "string",
