      "name": "exchange",
      "default": "",
      "optional": true,
      "control": "text",
      "type": "string",
      "hint": {
        "en_US": "The exchange to publish to.",
        "zh_CN": "要发送的交换器"
      },
      "label": {
        "en_US": "Exchange",
        "zh_CN": "交换器"
